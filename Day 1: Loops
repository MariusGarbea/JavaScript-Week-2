function vowelsAndConsonants(s) {
    for(var i = 0; i<s.length; i++)
        if(['a', 'e', 'i', 'o', 'u'].indexOf(s[i].toLowerCase()) !== -1){
            console.log(s[i]);
        }
    for(var i = 0; i<s.length; i++)
        if(['a', 'e', 'i', 'o', 'u'].indexOf(s[i].toLowerCase()) === -1){
            console.log(s[i]);
        }
}
